config_version=5

[application]

config/name="2d_space_shooter"
run/main_scene="res://entities/scenes/main.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.1", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"
config/size/window_height_override=0

[autoload]

SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
Env="*res://addons/godotenv/scripts/env.gd"
Utilities="*res://globals/utilities.gd"
Events="*res://globals/events.gd"

[display]

